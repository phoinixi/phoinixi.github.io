        background-color: var(--color-bg-secondary);
        margin-bottom: 0.75rem;
        overflow-x: auto;
        border-radius: 0.375rem;
        padding: 0.5rem;
      }

      .markdown pre code {
        background-color: transparent;
        padding: 0px;
      }

      .markdown blockquote {
        border-color: var(--color-border);
        margin-top: 0.75rem;
        margin-bottom: 0.75rem;
        border-left-width: 4px;
        padding-left: 1rem;
